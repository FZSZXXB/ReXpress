<%- include('util') %>
<!DOCTYPE html>
<html lang="zh-CN" style="position: fixed; width: 100%; overflow: hidden; ">
<head>
    <style>
        .ui.menu .item>i.fal {
        opacity: .9;
        float: none;
        margin: 0 .35714286em 0 0;
    }
    </style>
    <meta charset="utf-8">
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=1200">
    <title><%= title %> - 福州三中第 52 届田径运动会实时查分系统</title>
    <link href="https://cdn.jsdelivr.net/gh/FYMS-OI/cdn@2021-08-27/css/semantic.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/gh/Linhk1606/fontawesome-cdn@master/css/all.min.css" rel="stylesheet">
    <!-- <link href="/css/main.css", rel="stylesheet"> -->
    <link rel="shortcut icon" type="image/jpg" href="/img/fzsz.jpg"/>
    <script data-cfasync="false" src="<%- lib('jquery/3.3.1/jquery.min.js') %>"></script>
	<script src="https://cdn.jsdelivr.net/npm/sweetalert/dist/sweetalert.min.js"></script>
</head>
<body>
	<div id="root">
    <div class="ui fixed borderless menu" style="position: fixed; height: 49px; ">
			<div id="mainMenuContainer" class="ui container">
				<a class="header item" href="/"><img src="/img/fzsz.jpg" style="height:40px;">
				<img src="/img/fzsz-name.svg" style="width: 400px !important; margin-left: 1rem;">

				<div class="right menu">
				<% if (user) { %>
				<div class="ui simple dropdown item">
					<span class="user"><%= user.username %></span> <i class="fad fa-angle-down"></i>
					<div class="menu">
						<a class="item" href-post="<%= web_util.makeUrl(['loginPage', 'logout'], { url: '/' }) %>"><i class="fad fa-power-off"></i>注销</a>
					</div>
				</div>
				<% } else { %>
				<div class="item">
					<a class="ui button" style="margin-right: 0.5em; " href="<%= web_util.makeUrl(['loginPage'], { url: '/' }) %>">
						登录
					</a>
					<a class="ui primary button" href="<%= web_util.makeUrl(['registerPage'], { url: '/' }) %>">
						注册
					</a>
				</div>
				<% } %>
				</div>
			</div>
    </div>
	<div style="margin-top: 49px; height: calc(100% - 49px); overflow-y: auto;">
	<div style="margin-top: 28px; ">
	<div id="mainUiContainer" class="ui main container">


