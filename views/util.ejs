<%
this.getGradeName = (grade) => {
	switch (grade) {
		case 0:
			return '教工';
		case 1:
			return '高一';
		case 2:
			return '高二';
		case 3:
			return '高三';
		default:
			return undefined;
	}
};

this.getTypeName = (type) => {
	switch (type) {
		case 'stu_male':
			return '男子组';
		case 'stu_female':
			return '女子组';
		case 'stu_union':
			return '集体组';
		case 'teach_1':
			return '甲组';
		case 'teach_2':
			return '乙组';
		case 'teach_3':
			return '丙组';
		case 'teach_union':
			return '集体组';
		default:
			return undefined;
	}
};

this.getGradeColor = (grade) => {
	switch (grade) {
		case 0:
			return 'purple';
		case 1:
			return 'red';
		case 2:
			return 'orange';
		case 3:
			return 'blue';
		default:
			return undefined;
	}
};

this.getTypeColor = (type) => {
	switch (type) {
		case 'stu_male':
			return 'blue';
		case 'stu_female':
			return 'red';
		case 'stu_union':
			return 'orange';
		case 'teach_1':
			return 'purple';
		case 'teach_2':
			return 'cyan';
		case 'teach_3':
			return 'pink';
		case 'teach_union':
			return 'teal';
		default:
			return undefined;
	}
};

this.builtInCdnUrl = 'https://cdn.jsdelivr.net/npm/syzoj-public-cdn@1.1.0';

this.lib = uri => { return builtInCdnUrl + '/cdnjs/' + uri; }
this.selfLib = uri => { return builtInCdnUrl + '/self/' + uri; }
%> 