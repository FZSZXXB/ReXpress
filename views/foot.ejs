<script type="text/javascript">
	function copyText(text) {
		var $textarea = $('<textarea>');
		$textarea.text(text);
		$('body').append($textarea);
		$textarea.select();
		document.execCommand('copy');
		$textarea.remove();
		swal({
			title: "复制",
			icon: "success",
			text: "内容已复制到剪贴板。"
		});
	}
	$('body').on('click', '*[data-click-copy]', function () {
		var text = $(this).attr('data-click-copy');
		copyText(text);
	});
</script>
